@import './styles/iconfont.wxss';
/* 微信小程序中不支持通配符选择器 */
page,view,text,swiper,swiper-item,image,navigater{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

page{
    /* 定义主题颜色 */
    --themeColor:#eb4450;
    /* 统一字体大小   假设设计稿是375px */
    /* 1px=2rpx */
    /* 14px=28rpx */
    font-size: 28rpx;
}
image{
    width: 100%;
}
